{
	"ecmaFeatures": {
		"jsx": true
	},
	"parser": "babel-eslint",
	"rules": {
		"yoda": 0,
		"no-underscore-dangle": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-multi-spaces": 0,
		"global-strict": false,
		"no-extra-strict": 0,
		"strict": [2, "global"],
		"no-console": 0,
		"quotes": 0,
		"no-shadow": 0,
		"no-unused-vars": [2, {"vars": "local"}],
	}
}
